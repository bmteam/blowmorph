<?xml version="1.0" encoding="UTF-8"?>

<!--
When using 'grid' map all coordinates will be multiplied by 'block_size'.
-->

<map type="grid" block_size="16.0" bound="600.0">
  <spawn x="0" y="0"></spawn>
  <spawn x="-7" y="-7"></spawn>

  <wall x="6" y="6" type="unbreakable"></wall>
  <wall x="7" y="6" type="unbreakable"></wall>
  <wall x="8" y="6" type="morphed"></wall>
  <wall x="9" y="6" type="ordinary"></wall>

  <chunk x="11" y="0" width="10" height="10" type="ordinary"></chunk>
</map>

